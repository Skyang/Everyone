<div id="doc">
    <% include ./navbar.ejs %>
    <div id="indexCarousel" class="carousel slide" data-ride="carousel" data-interval="3000">
        <!-- 轮播（Carousel）指标 -->
        <ol class="carousel-indicators">
            <li data-target="#indexCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#indexCarousel" data-slide-to="1"></li>
            <li data-target="#indexCarousel" data-slide-to="2"></li>
            <li data-target="#indexCarousel" data-slide-to="3"></li>
            <li data-target="#indexCarousel" data-slide-to="4"></li>
        </ol>
        <!-- 轮播（Carousel）项目 -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="/images/slider/slider_0.jpg" alt="First slide">
            </div>
            <div class="item">
                <img src="/images/slider/slider_1.jpg" alt="Second slide">
            </div>
            <div class="item">
                <img src="/images/slider/slider_2.jpg" alt="Third slide">
            </div>
            <div class="item">
                <img src="/images/slider/slider_3.jpg" alt="Fourth slide">
            </div>
            <div class="item">
                <img src="/images/slider/slider_4.jpg" alt="Fifth slide">
            </div>
        </div>
        <!-- 轮播（Carousel）导航 -->
        <a class="carousel-control left" href="#indexCarousel"
           data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#indexCarousel"
           data-slide="next">&rsaquo;</a>
    </div>
</div>